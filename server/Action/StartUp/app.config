<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="socketServer" type="SuperSocket.SocketEngine.Configuration.SocketServiceConfig, SuperSocket.SocketEngine" />
  </configSections>
  <appSettings>
    <add key="ServiceName" value="SupperSocketService" />
  </appSettings>
  <connectionStrings/>
  <socketServer loggingMode="IndependantFile">
    <servers>
      <server name="hydGameServer"
              serviceName="hydGameService"
              ip="Any" port="2020"
              mode="Async"
              maxConnectionNumber="100">
      </server>
    </servers>
    <services>
      <service name="hydGameService1" type="Action.Engine.GameServer, Action.Engine" />
    </services>
  </socketServer>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0" />
  </startup>
</configuration>
